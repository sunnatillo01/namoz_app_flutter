[{"id":"6198","sura":"107","aya":"1","arabic_text":"أَرَءَيۡتَ ٱلَّذِي يُكَذِّبُ بِٱلدِّينِ","translation":"Охират(жазоси)ни ёлғонга чиқарувчини кўрдингми?","footnotes":""},{"id":"6199","sura":"107","aya":"2","arabic_text":"فَذَٰلِكَ ٱلَّذِي يَدُعُّ ٱلۡيَتِيمَ","translation":"У, етимни қўполлик билан ҳайдайдир.","footnotes":""},{"id":"6200","sura":"107","aya":"3","arabic_text":"وَلَا يَحُضُّ عَلَىٰ طَعَامِ ٱلۡمِسۡكِينِ","translation":"Ва мискинларга таом беришга қизиқтирмайдир.","footnotes":""},{"id":"6201","sura":"107","aya":"4","arabic_text":"فَوَيۡلٞ لِّلۡمُصَلِّينَ","translation":"Бир «намозхонлар»га вайл бўлсинки. ","footnotes":""},{"id":"6202","sura":"107","aya":"5","arabic_text":"ٱلَّذِينَ هُمۡ عَن صَلَاتِهِمۡ سَاهُونَ","translation":"Улар намозларини унутувчилардир.","footnotes":""},{"id":"6203","sura":"107","aya":"6","arabic_text":"ٱلَّذِينَ هُمۡ يُرَآءُونَ","translation":"Ундоқ кишилар риё қиладиганлардир.","footnotes":""},{"id":"6204","sura":"107","aya":"7","arabic_text":"وَيَمۡنَعُونَ ٱلۡمَاعُونَ","translation":"Ва мааъуунни ман қиладиганлардир.","footnotes":""}]