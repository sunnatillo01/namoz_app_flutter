[{"id":"6231","sura":"114","aya":"1","arabic_text":"قُلۡ أَعُوذُ بِرَبِّ ٱلنَّاسِ","translation":"«Одамлар Роббисидан...","footnotes":""},{"id":"6232","sura":"114","aya":"2","arabic_text":"مَلِكِ ٱلنَّاسِ","translation":"Одамлар подшоҳидан...","footnotes":""},{"id":"6233","sura":"114","aya":"3","arabic_text":"إِلَٰهِ ٱلنَّاسِ","translation":"Одамлар Илоҳидан паноҳ сўрайман...","footnotes":""},{"id":"6234","sura":"114","aya":"4","arabic_text":"مِن شَرِّ ٱلۡوَسۡوَاسِ ٱلۡخَنَّاسِ","translation":"Беркиниб, кўриниб турувчи васвасачининг... ","footnotes":""},{"id":"6235","sura":"114","aya":"5","arabic_text":"ٱلَّذِي يُوَسۡوِسُ فِي صُدُورِ ٱلنَّاسِ","translation":"Одамлар қалбларига васваса соладиганнинг... ","footnotes":""},{"id":"6236","sura":"114","aya":"6","arabic_text":"مِنَ ٱلۡجِنَّةِ وَٱلنَّاسِ","translation":"Жинлар ва одамлардан бўлганнинг ёмонлигидан», деб айт.","footnotes":"(Оятда зикр қилинган «беркиниб, кўриб турувчи»дан мурод шайтондир.)"}]