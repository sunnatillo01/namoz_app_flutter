[{"id":"6205","sura":"108","aya":"1","arabic_text":"إِنَّآ أَعۡطَيۡنَٰكَ ٱلۡكَوۡثَرَ","translation":"Албатта, Биз сенга Кавсарни бердик.","footnotes":""},{"id":"6206","sura":"108","aya":"2","arabic_text":"فَصَلِّ لِرَبِّكَ وَٱنۡحَرۡ","translation":"Бас, Роббингга намоз ўқи ва жонлиқ сўй.","footnotes":""},{"id":"6207","sura":"108","aya":"3","arabic_text":"إِنَّ شَانِئَكَ هُوَ ٱلۡأَبۡتَرُ","translation":"Албатта, сени ёмон кўриб айбловчининг ўзининг орқаси кесик.","footnotes":""}]