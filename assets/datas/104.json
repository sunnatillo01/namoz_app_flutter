[{"id":"6180","sura":"104","aya":"1","arabic_text":"وَيۡلٞ لِّكُلِّ هُمَزَةٖ لُّمَزَةٍ","translation":"Ҳар бир обрў тўкувчи ва айбловчига «вайл» бўлсин.","footnotes":""},{"id":"6181","sura":"104","aya":"2","arabic_text":"ٱلَّذِي جَمَعَ مَالٗا وَعَدَّدَهُۥ","translation":"У мол жамлади ва уни санаб турур. ","footnotes":""},{"id":"6182","sura":"104","aya":"3","arabic_text":"يَحۡسَبُ أَنَّ مَالَهُۥٓ أَخۡلَدَهُۥ","translation":"У, албатта, моли уни абадий қолдирур деб ҳисоб қилур.","footnotes":""},{"id":"6183","sura":"104","aya":"4","arabic_text":"كَلَّاۖ لَيُنۢبَذَنَّ فِي ٱلۡحُطَمَةِ","translation":"Йўқ! Албатта, у «ҳутома»га хор-зор этиб ташланур.","footnotes":""},{"id":"6184","sura":"104","aya":"5","arabic_text":"وَمَآ أَدۡرَىٰكَ مَا ٱلۡحُطَمَةُ","translation":"«Ҳутома» қандоқ нарса эканини сенга нима билдирур?","footnotes":""},{"id":"6185","sura":"104","aya":"6","arabic_text":"نَارُ ٱللَّهِ ٱلۡمُوقَدَةُ","translation":"У, Аллоҳнинг шиддатли, ўчмас оловидир. ","footnotes":""},{"id":"6186","sura":"104","aya":"7","arabic_text":"ٱلَّتِي تَطَّلِعُ عَلَى ٱلۡأَفۡـِٔدَةِ","translation":"Қалбларгача етиб борадигандур. ","footnotes":""},{"id":"6187","sura":"104","aya":"8","arabic_text":"إِنَّهَا عَلَيۡهِم مُّؤۡصَدَةٞ","translation":"Албатта, у уларнинг устидан қоплангандир. ","footnotes":""},{"id":"6188","sura":"104","aya":"9","arabic_text":"فِي عَمَدٖ مُّمَدَّدَةِۭ","translation":"Узун-узун устунларга боғлангандир.","footnotes":""}]